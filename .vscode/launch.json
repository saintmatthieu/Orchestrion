{
  "configurations": [
    {
      "name": "Debug Qt Application with Visual Studio Debugger",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/${workspaceFolderBasename}.exe",
      "preLaunchTask": "build",
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "visualizerFile": "${workspaceFolder}/qt6.natvis",
      "sourceFileMap": {
        "C:/Users/qt/work/qt": "${QTDIR}/../../Src"
      },
    }
  ]
}